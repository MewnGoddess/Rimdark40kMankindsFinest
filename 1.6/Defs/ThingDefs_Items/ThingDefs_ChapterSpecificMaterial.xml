    <!-- Base -->
    <ThingDef Name="BEWH_ChapterGeneticMaterial" Abstract="True">
        <thingClass>Genes40k.GeneMaterialExtra</thingClass>
        <statBases>
            <MaxHitPoints>200</MaxHitPoints>
            <MarketValue>4000</MarketValue>
            <Mass>0.5</Mass>
            <Flammability>0</Flammability>
        </statBases>
        <thingCategories>
            <li>BEWH_ChapterMaterial</li>
        </thingCategories>
        <tradeTags>
            <li>BEWH_ChapterMaterial</li>
        </tradeTags>
        <stackLimit>1</stackLimit>
        <category>Item</category>
        <drawerType>MapMeshOnly</drawerType>
        <resourceReadoutPriority>Middle</resourceReadoutPriority>
        <selectable>true</selectable>
        <altitudeLayer>Item</altitudeLayer>
        <alwaysHaulable>true</alwaysHaulable>
        <drawGUIOverlay>true</drawGUIOverlay>
        <rotatable>false</rotatable>
        <pathCost>14</pathCost>
        <thingSetMakerTags>
            <li>RewardStandardHighFreq</li>
        </thingSetMakerTags>
        <descriptionHyperlinks>
            <ThingDef>BEWH_SangprimusPortum</ThingDef>
        </descriptionHyperlinks>
        <comps>
            <li Class="CompProperties_Forbiddable"/>
        </comps>
    </ThingDef>

    <!-- Legion I - Dark Angels -->
    <ThingDef ParentName="BEWH_ChapterGeneticMaterial">
        <defName>BEWH_ChapterMaterialI</defName>
        <label>legion material I - Dark Angels</label>
        <description>A specialized container designed to store and preserve the sacred genetic data of a specific Astartes Legion. Legion material develops within a Space Marine's body and can be harvested once fully developed and retrieved from the fallen.\n\nThis unit holds the genetic mutation material of Legion I - Dark Angels.</description>
        <graphicData>
            <texPath>Things/Item/ChapterMaterial/BEWH_ChapterMaterial_I</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <descriptionHyperlinks>
            <GeneDef>BEWH_ChapterIDarkAngels</GeneDef>
        </descriptionHyperlinks>
        <modExtensions>
            <li Class="Genes40k.DefModExtension_ChapterMaterial">
                <orderInt>1</orderInt>
                <shownMaterialName>Dark Angels</shownMaterialName>
            </li>
            <li Class="Genes40k.DefModExtension_GeneFromMaterial">
                <addedGene>BEWH_ChapterIDarkAngels</addedGene>
            </li>
        </modExtensions>
    </ThingDef>

