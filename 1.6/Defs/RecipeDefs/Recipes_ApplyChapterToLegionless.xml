<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <RecipeDef Name="BEWH_LegionlessToLegion" ParentName="SurgeryFlesh" Abstract="True">
        <description>Implant legion material into a patient.</description>
        <jobString>Implanting legion material.</jobString>
        <researchPrerequisite>BEWH_GeneseedPurification</researchPrerequisite>
        <workAmount>3500</workAmount>
        <workerClass>Genes40k.WorkerClass_ImplantLegionMaterial</workerClass>
        <hideBodyPartNames>true</hideBodyPartNames>
        <targetsBodyPart>false</targetsBodyPart>
        <surgeryOutcomeEffect Inherit = "False" />
        <surgerySuccessChanceFactor>3</surgerySuccessChanceFactor>
        <isViolation>true</isViolation>
        <recipeUsers>
            <li>Human</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>MedicineUltratech</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>MedicineUltratech</li>
            </thingDefs>
        </fixedIngredientFilter>
        <skillRequirements>
            <Medicine>9</Medicine>
        </skillRequirements>
    </RecipeDef>

    <!-- Legion I -->
    <RecipeDef ParentName="BEWH_LegionlessToLegion">
        <defName>BEWH_IntroduceMaterialChapterI</defName>
        <label>introduce Dark Angels material</label>
        <modExtensions>
            <li Class="Genes40k.DefModExtension_LegionMaterialCreation">
                <requiredLegionMaterial>BEWH_ChapterMaterialI</requiredLegionMaterial>
            </li>
        </modExtensions>
    </RecipeDef>

