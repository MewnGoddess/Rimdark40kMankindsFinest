<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Primaris -->
    <RecipeDef ParentName="BEWH_RecipeGeneseedVialBase" Name="BEWH_PrimarisVialInsertion" Abstract="True">
        <description>Implant Primaris genetic material into a patient.</description>
        <jobString>Implanting Primaris genetic material.</jobString>
        <researchPrerequisite>BEWH_GeneseedPerfection</researchPrerequisite>
        <workAmount>6500</workAmount>
        <uiIconThing>BEWH_GeneseedVialPrimaris</uiIconThing>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>MedicineUltratech</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>BEWH_GeneseedVialPrimaris</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>BEWH_GeneseedVialPrimaris</li>
            </thingDefs>
        </fixedIngredientFilter>
        <skillRequirements>
            <Medicine>10</Medicine>
        </skillRequirements>
    </RecipeDef>

    <!-- No Chapter -->
    <RecipeDef ParentName="BEWH_PrimarisVialInsertion">
        <defName>BEWH_MakePrimaris</defName>
        <label>make chapterless primaris</label>
        <modExtensions>
            <li Class="Genes40k.DefModExtension_GeneseedVialRecipe">
                <geneseedVial>BEWH_GeneseedVialPrimaris</geneseedVial>
            </li>
        </modExtensions>
    </RecipeDef>

