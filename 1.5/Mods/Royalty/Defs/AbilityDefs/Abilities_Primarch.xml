    <iconPath>UI/Abilities/BEWH_Ability_LemanRussDTW</iconPath>
    <cooldownTicksRange>30000</cooldownTicksRange>
    <aiCanUse>true</aiCanUse>
    <category>BEWH_Primarch</category>
