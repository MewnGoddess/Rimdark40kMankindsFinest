    <defName>BEWH_PrimarchMaterialI</defName>
    <label>primarch material I - Lion El'Jonson</label>
    <description>This containers holds the genetic material of Primarch I - Lion El'Jonson.</description>
    <graphicData>
      <texPath>Things/Item/PrimarchMaterial/BEWH_PrimarchMaterial_I</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <modExtensions>
      <li Class="Genes40k.DefModExtension_PrimarchMaterial" />
      <li Class="Genes40k.DefModExtension_GeneFromMaterial">
        <addedGene>BEWH_PrimarchSpecificGeneI</addedGene>
      </li>
    </modExtensions>
  </ThingDef>

