        <appliesHediff>BEWH_FirstbornPhaseOne</appliesHediff>
        <minAgeImplant>10</minAgeImplant>
        <maxAgeImplant>12</maxAgeImplant>
        <failureChancePerAgePast>5</failureChancePerAgePast>
        <failChanceCap>85</failChanceCap>
        <baseFailureChance>10</baseFailureChance>
      </li>
    </modExtensions>
  </ThingDef>

