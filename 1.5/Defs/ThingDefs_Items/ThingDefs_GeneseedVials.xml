      <drawSize>(0.85, 0.85)</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>10000</MarketValue>
    </statBases>
    <modExtensions>
      <li Class="Genes40k.DefModExtension_GeneseedVial">
        <xenotype>BEWH_SpaceMarine</xenotype>
        <xenotypeIcon>BEWH_AstartesIcon</xenotypeIcon>
        <recipe>BEWH_MakeFirstborn</recipe>
        <overrideXenotypeGenesGiven>true</overrideXenotypeGenesGiven>
        <overridenAddedGenes>
          <li>BEWH_SecondaryHeart</li>
          <li>BEWH_Ossmodula</li>
          <li>BEWH_Biscopea</li>
          <li>BEWH_Haemastamen</li>
          <li>BEWH_LarramansOrgan</li>
