<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- These have been gestated from matrices and are now ready to be implanted into a pawn-->

  <!-- Thunder Warrior -->
  <ThingDef ParentName="BEWH_GeneseedVialBase">
    <defName>BEWH_GeneseedVialThunderWarrior</defName>
    <label>thunder warrior geneseed vial</label>
    <description>This vial contains the gestated genes ready for implantation into a pawn, creating a Thunder Warrior.\n\nThe safe implantation age is 12~21, implantation into anyone outside this range may kill them.\n\nThis item rapidly deteriorates outside containment.</description>
    <graphicData>
      <texPath>Things/Item/GeneseedVial/GeneseedVial_ThunderWarrior</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(0.85, 0.85)</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>6500</MarketValue>
    </statBases>
    <modExtensions>
      <li Class="Genes40k.DefModExtension_GeneseedVial">
        <xenotype>BEWH_ThunderWarrior</xenotype>
        <xenotypeIcon>BEWH_ThunderWarriorIcon</xenotypeIcon>
        <recipe>BEWH_MakeThunderWarrior</recipe>
        <minAgeImplant>12</minAgeImplant>
        <maxAgeImplant>21</maxAgeImplant>
        <failureChancePerAgePast>3</failureChancePerAgePast>
        <failChanceCap>80</failChanceCap>
        <baseFailureChance>5</baseFailureChance>
      </li>
    </modExtensions>
  </ThingDef>

