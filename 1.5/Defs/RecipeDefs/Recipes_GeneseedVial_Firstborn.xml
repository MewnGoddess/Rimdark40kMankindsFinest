<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Firstborn -->
    <RecipeDef ParentName="BEWH_RecipeGeneseedVialBase" Name="BEWH_FirstbornVialInsertion" Abstract="True">
        <description>Implant Firstborn genetic material into a patient.</description>
        <jobString>Implanting Firstborn genetic material.</jobString>
        <researchPrerequisite>BEWH_GeneseedPurification</researchPrerequisite>
        <workAmount>5000</workAmount>
        <uiIconThing>BEWH_GeneseedVialFirstborn</uiIconThing>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>MedicineUltratech</li>
                    </thingDefs>
                </filter>
                <count>8</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>BEWH_GeneseedVialFirstborn</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>BEWH_GeneseedVialFirstborn</li>
            </thingDefs>
        </fixedIngredientFilter>
        <skillRequirements>
            <Medicine>8</Medicine>
        </skillRequirements>
    </RecipeDef>

    <!-- No Chapter -->
    <RecipeDef ParentName="BEWH_FirstbornVialInsertion">
        <defName>BEWH_MakeFirstborn</defName>
        <label>make chapterless firstborn</label>
        <modExtensions>
            <li Class="Genes40k.DefModExtension_GeneseedVialRecipe">
                <geneseedVial>BEWH_GeneseedVialFirstborn</geneseedVial>
            </li>
        </modExtensions>
    </RecipeDef>

