<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Iota Psyker -->
    <GeneDef ParentName="BEWH_PsykerParent">
        <defName>BEWH_IotaPsyker</defName>
        <label>iota psyker</label>
        <description>Carriers of this gene are able to unlock their psychic potential, though it is only a sliver of what's possible.</description>
        <iconPath>UI/Genes/Psyker/BEWH_GenePsykerIota</iconPath>
        <displayOrderInCategory>600</displayOrderInCategory>
        <biostatCpx>1</biostatCpx>
        <biostatMet>-1</biostatMet>
        <statFactors>
            <PsychicSensitivity>1.15</PsychicSensitivity>
            <MeditationFocusGain>1.05</MeditationFocusGain>
            <PsychicEntropyRecoveryRate>1.05</PsychicEntropyRecoveryRate>
        </statFactors>
        <capMods>
            <li>
                <capacity>Consciousness</capacity>
                <postFactor>1.05</postFactor>
            </li>
        </capMods>
        <marketValueFactor>2</marketValueFactor>
    </GeneDef>

