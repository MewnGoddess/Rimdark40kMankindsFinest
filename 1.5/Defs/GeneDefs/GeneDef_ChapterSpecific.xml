    <!-- Chaper VI - Space Wolves -->
    <GeneDef ParentName="BEWH_ChapterGeneParent">
        <defName>BEWH_ChapterVISpaceWolves</defName>
        <label>Geneseed Material: Space Wolves</label>
        <description>Your geneseeds comes from</description>
        <iconPath>UI/Genes/ChaperVISpaceWolves</iconPath>
        <displayOrderInCategory>115</displayOrderInCategory>
        <minAgeActive>14</minAgeActive>
        <geneClass>Genes40k.Gene_AddGeneByChance</geneClass>
        <capMods>
            <li>
                <capacity>Hearing</capacity>
                <offset>1.3</offset>
            </li>
            <li>
                <capacity>Sight</capacity>
                <offset>1.1</offset>
            </li>
        </capMods>
        <statFactors>
            <IncomingDamageFactor>0.95</IncomingDamageFactor>
        </statFactors>
        <statOffsets>
            <ComfyTemperatureMin>-15</ComfyTemperatureMin>
        </statOffsets>
        <hairColorOverride>(0.65, 0.65, 0.65)</hairColorOverride>
        <modExtensions>
            <li Class="Genes40k.DefModExtension_ChapterGene" />
            <li Class="Genes40k.DefModExtension_AddGeneByChance">
                <possibleGenesToGive>
                    <li>
                        <key>BEWH_ChapterVIFlaw</key>
                        <value>100</value>
                    </li>
                </possibleGenesToGive>
            </li>
        </modExtensions>
    </GeneDef>
    <!-- Chapter VI - Flaw -->
    <GeneDef ParentName="BEWH_ChapterGeneFlawParent">
        <defName>BEWH_ChapterVIFlaw</defName>
        <label>Canis Helix</label>
        <description>The Canis Helix is a mutation of normal gene-seed that gifts the Wolves with enhanced abilities, including hyper-acute senses of smell and hearing, elongated canines that can dent plasteel, and leathery, resilient skin.</description>
        <iconPath>UI/Genes/ChaperVISpaceWolvesFlaw</iconPath>
        <displayOrderInCategory>116</displayOrderInCategory>
        <minAgeActive>14</minAgeActive>
        <capMods>
            <li>
                <capacity>Hearing</capacity>
                <postFactor>1.4</postFactor>
            </li>
            <li>
                <capacity>Sight</capacity>
                <postFactor>1.2</postFactor>
            </li>
        </capMods>
        <socialFightChanceFactor>1.25</socialFightChanceFactor>
        <aggroMentalBreakSelectionChanceFactor>999</aggroMentalBreakSelectionChanceFactor>
        <customEffectDescriptions>
            <li>Curse of the Wulfen</li>
        </customEffectDescriptions>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <hediffsToBodyParts>
                    <li>
                        <hediff>BEWH_CanisHelixCanines</hediff>
                        <bodyparts>
                            <li>Head</li>
                        </bodyparts>
                    </li>
                </hediffsToBodyParts>
            </li>
        </modExtensions>
    </GeneDef>

