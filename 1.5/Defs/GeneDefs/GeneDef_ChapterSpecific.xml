    <!-- Chaper XV - Thousand Sons -->
    <GeneDef ParentName="BEWH_ChapterGeneParent">
        <defName>BEWH_ChapterXVThousandSons</defName>
        <label>Geneseed Material: Thousand Sons</label>
        <description>Your geneseeds comes from Magnus the Red, the Crimson King, and as such you have inherited some of his psychic potential.</description>
