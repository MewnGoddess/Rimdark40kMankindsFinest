        <displayOrderInCategory>403</displayOrderInCategory>
        <minAgeActive>14</minAgeActive>
        <statFactors>
            <SocialImpact>2.5</SocialImpact>
            <ConversionPower MayRequire="Ludeon.RimWorld.Ideology">2</ConversionPower>
        </statFactors>
        <forcedTraits>
            <li>
                <def>Beauty</def>
                <degree>2</degree>
